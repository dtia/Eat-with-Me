function showBio(user_id, bio) {
	$("#description" + user_id).html(bio);
	$("#history" + user_id).removeClass("active");
	$("#bio" + user_id).addClass("active");
}

function showRestaurantHistory(user_id) {
	$("#description" + user_id).html("I've eaten at these other restaurants:");
	$("#bio" + user_id).removeClass("active");
	$("#history" + user_id).addClass("active");
}
